    <connection id="6ac18ace-a89a-4e41-b85c-2edcb1854104">
      <console name="23-01-26_financials" type="STANDARD" schema="main" session="Main">SELECT * FROM companies

SELECT ID FROM companies WHERE symbol = &quot;AAPL&quot; AND name = &quot;Apple Inc.&quot;;

SELECT * FROM income_statement_rows WHERE key IS 1
SELECT * FROM income_statement_cells WHERE key IS 1

SELECT * FROM balance_sheet_rows WHERE key IS 8
SELECT * FROM balance_sheet_cells

SELECT * FROM cash_flow_rows WHERE key IS 8
SELECT * FROM cash_flow_cells WHERE key IS 1350

-------------------------------

DELETE FROM companies
DELETE FROM income_statement_rows
DELETE FROM income_statement_cells
DELETE FROM balance_sheet_rows
DELETE FROM balance_sheet_cells
DELETE FROM cash_flow_rows
DELETE FROM cash_flow_cells

-------------------------------

-- Income Statement
SELECT c.ID, c.symbol, c.name, c.industry, c.country, c.exchange, 
       isr.row_number, isr.data_item, 
       isc.period_end, isc.information 
FROM companies AS c
INNER JOIN income_statement_rows AS isr ON c.ID = isr.key
INNER JOIN income_statement_cells AS isc ON isr.ID = isc.key
WHERE c.ID = 5175
ORDER BY c.ID ASC, isr.row_number ASC

-- Balance Sheet
SELECT c.ID, c.symbol, c.name, c.industry, c.country, c.exchange, 
       bsr.row_number, bsr.data_item, 
       bsc.period_end, bsc.information 
FROM companies AS c
INNER JOIN balance_sheet_rows AS bsr ON c.ID = bsr.key
INNER JOIN balance_sheet_cells AS bsc ON bsr.ID = bsc.key
WHERE c.ID = 50
ORDER BY c.ID ASC, bsr.row_number ASC

-- Cash Flow
SELECT c.ID, c.symbol, c.name, c.industry, c.country, c.exchange, 
       cfr.row_number, cfr.data_item, 
       cfc.period_end, cfc.information 
FROM companies AS c
INNER JOIN cash_flow_rows AS cfr ON c.ID = cfr.key
INNER JOIN cash_flow_cells AS cfc ON cfr.ID = cfc.key
WHERE c.ID = 50
ORDER BY c.ID ASC, cfr.row_number ASC</console>
    </connection>
